POST     /garages                         controllers.GarageController.createGarage
GET      /garages/:garageId               controllers.GarageController.retrieveGarage(garageId)
PUT      /garages/:garageId               controllers.GarageController.modifyGarage(garageId)
DELETE   /garages/:garageId               controllers.GarageController.deleteGarage(garageId)
GET      /garages/:garageId/cars          controllers.GarageController.retrieveAllCars(garageId)
POST     /garages/:garageId/cars          controllers.GarageController.addCarToGarage(garageId)
DELETE   /garages/:garageId/cars          controllers.GarageController.deleteAllCarsOfGarage(garageId)
# GET      /garages/:garageId/cars/:carId   controllers.GarageController.retrieveCarFromGarage(carId)
# PUT      /garages/:garageId/cars/:carId   controllers.GarageController.modifyCarMatriculation(carId)
# DELETE   /garages/:garageId/cars/:carId   controllers.GarageController.deleteCarFromGarage(carId)