POST     /garages                         controllers.GarageController.createGarage
GET      /garages/:garageId               controllers.GarageController.retrieveGarage(garageId: java.util.UUID)
PUT      /garages/:garageId               controllers.GarageController.modifyGarage(garageId: java.util.UUID)
DELETE   /garages/:garageId               controllers.GarageController.deleteGarage(garageId: java.util.UUID)
GET      /garages/:garageId/cars          controllers.GarageController.retrieveAllCars(garageId: java.util.UUID)
POST     /garages/:garageId/cars          controllers.GarageController.addCarToGarage(garageId: java.util.UUID)
DELETE   /garages/:garageId/cars          controllers.GarageController.deleteAllCarsOfGarage(garageId: java.util.UUID)
GET      /garages/:garageId/cars/:carId   controllers.GarageController.retrieveCarFromGarage(garageId: java.util.UUID, carId: java.util.UUID)
PUT      /garages/:garageId/cars/:carId   controllers.GarageController.modifyCarMatriculation(garageId: java.util.UUID, carId: java.util.UUID)
DELETE   /garages/:garageId/cars/:carId   controllers.GarageController.deleteCarFromGarage(garageId: java.util.UUID, carId: java.util.UUID)